  "files": [
    "problematic.json",
    "index.js",
    "cli.js"
  ],
  "bin": {
    "syllable": "cli.js"
  },
  "dependencies": {
    "has": "^1.0.1",
    "normalize-strings": "^1.1.0",
    "pluralize": "^3.0.0",
    "trim": "0.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
